{
    "manifest_version": 3,
    "name": "Title Fetcher",
    "version": "1.0",
    "description": "Fetches and displays the title of the current tab.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": [
      "tabs"
    ]
  }
  